  @model List<YSWL.MALL.Model.Shop.Supplier.SupplierAD>
@if (Model != null && Model.Count > 0)
{
    <div class="bannerWrapper swiper-container">
        <div class="swiper-wrapper">
            @foreach (var item in Model)
            {
            <a class="swiper-slide" href="@item.NavigateUrl"><img src="@item.FileUrl" alt="@item.Name"></a>
            }
        </div>
        <div class="swiper-pagination"></div>
    </div>
    <div class="store-home-banner1-line"></div>
}

