@model List<YSWL.MALL.Model.CMS.ContentClass>
    <div class="clearfix article_nav ">
        <div class="left_common">
             @if (Model == null || Model.Count == 0)
             {
                 <dl class="left_help">
                     <dt>@ViewBag.AclassName</dt>
                     <dd>
                           @Html.Action("ContentTitleList", "Article", new { classid = ViewBag.AclassId })
                     </dd>
                 </dl>
             }
             else
             {
                   <div class="helpCenter-title f14 fb" style="display:none;">
                      @ViewBag.AclassName</div>
                 for (int i = 0; i < Model.Count; i++)
                 {
                     <dl class="left_help">
                         <dt >@Model[i].ClassName</dt>
                         <dd>
                             @Html.Action("ContentTitleList", "Article", new { classid = Model[i].ClassID })
                         </dd>
                     </dl>
                 }
             }
        </div>
    </div>
