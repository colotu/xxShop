@{
    Layout = "~/Areas/CMS/Themes/Default/Views/Shared/_CMSLayout.cshtml";
}
<script src="/Scripts/jquery.cookie.js" type="text/javascript"></script>
@using YSWL.Common;
@model YSWL.MALL.Model.CMS.Content
<div class="main">
    <!--MainMenu Start-->
    @Html.Action("MainMenu", "Partial")
    <!--MainMenu End-->
    <div class="main_container">
        <div class="container_sidebar">
            <div class="container_sidebar_a">
                <div class="detailed2">
                    <center>
                        <h4><b>
                            关于读科技</b></h4>
                    </center>
                    <ul>
                    </ul>
                </div>
                <div class="detailed_cont">
                    <ul>
                        <li>
                            <p>读科技（dukeji.com）是一家专注于科技资讯与网络新闻的媒体网站，内容包括行业资讯，电子商务，社交网络，移动互联网，创业投资，产品设计，最新技术等内容，聚合了业界最新最热的观点和文章，为创业者寻求新的突破提供知识源泉，同时也为新产品的曝光宣传提供一个资讯发布平台。该网站面向众多的科技工作者，IT人士，互联网创业者及科技教育者，所有喜欢阅读的各种人士。在社会化与数字阅读的理念指导下，未来将打造一个以兴趣为中心，为用户提供个性化信息聚合与阅读分享社会化媒体。</p>
                            <p>联系我们： market@dukeji.com</p>
                        </li>
                    </ul>
                </div>
                <div class="detailed_button">
                </div>
                <div class="detailed_prev">
                    <p class="detailed_prev_sp">
                    </p>
                    <p class="detailed_prev_sp" style="float: right;">
                    </p>
                    <br />
                    <br />
                </div>
            </div>
            <div style="clear: both;">
            </div>
        </div>
        <div class="container_rightside">
            <!--无觅热门文章插件 -->
            <div class="comments">
                <h3>
                    热门文章</h3>
                <div style="margin-left: 5px; margin-right: 8px; margin-top: 10px;">
                    <!-- 无觅 BEGIN -->
                    <iframe width="100%" height="305" frameborder="0" scrolling="no" src="http://app.wumii.com/ext/widget/hot?prefix=http%3A%2F%2Fwww.dukeji.com&num=9&t=1">
                    </iframe>
                    <!-- 无觅 END -->
                </div>
            </div>
            <!--无觅热门文章插件end -->
            <!--FriendLink Start-->
            @Html.Action("FriendLink", "Partial")
            <!--FriendLink End-->
            <div class="ad">
                @Html.Action("ADDefindCode", "Partial", new { AdvPositionId = 16 })
            </div>
        </div>
        <div style="clear: both">
        </div>
    </div>
    <div style="clear: both;">
    </div>
</div>
<!--无觅相关文章插件 -->
<script type="text/javascript">
    var wumiiPermaLink = ""; //请用代码生成文章永久的链接
    var wumiiTitle = ""; //请用代码生成文章标题
    var wumiiTags = ""; //请用代码生成文章标签，以英文逗号分隔，如："标签1,标签2"
    var wumiiSitePrefix = "http://www.dukeji.com/";
    var wumiiParams = "&num=5&mode=3&pf=JAVASCRIPT";
</script>
<script type="text/javascript" src="http://widget.wumii.com/ext/relatedItemsWidget"></script>
<a href="http://www.wumii.com/widget/relatedItems" style="border: 0;">
    <img src="http://static.wumii.cn/images/pixel.png" alt="读科技" style="border: 0; padding: 0;
        margin: 0;" />
</a>
<!--无觅相关文章插件end -->
